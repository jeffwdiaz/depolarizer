# Node.js dependencies
node_modules/
frontend/node_modules/
backend/node_modules/
.cursor/** 

# Output directories from various hosts/builds
.output        # Generic output directory (some hosts)
.vercel        # Vercel deployment output
.netlify       # Netlify deployment output
.wrangler      # Cloudflare Wrangler output
/.svelte-kit   # SvelteKit build output (legacy, now in frontend/)
/frontend/.svelte-kit   # SvelteKit build output (frontend)
/build         # Production build output
/frontend/build         # Frontend production build output

# OS-specific files
.DS_Store      # macOS Finder metadata
Thumbs.db      # Windows Explorer thumbnail cache

# Environment variable files
.env           # Main environment file
.env.*         # All env files (e.g., .env.local, .env.production)
!.env.example  # Allow tracked example env file
!.env.test     # Allow tracked test env file

# Vite-specific timestamped config files
vite.config.js.timestamp-*   # Vite JS config with timestamp
vite.config.ts.timestamp-*   # Vite TS config with timestamp
